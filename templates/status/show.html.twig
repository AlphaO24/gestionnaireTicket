{% extends 'base.html.twig' %}
{% block title %} Status {% endblock %}
{% block body %}
<table style="border-collapse: collapse; width: 100%;" border="1">
<tbody>
<tr>
<td>Id</td>
<td>Name</td>
</tr>
{% for status_info in status%}
<tr>
<td> {{ status_info.id }}</td>
<td> {{ status_info.name }}</td>
</tr>
{% endfor %}
</tbody>
</table>
{% endblock %}
